{
  "name": "YDictionary",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Your lovely dictionary",
  "icons": {
   "64": "image/dictionary.png"
  },
  "browser_action": {
    "default_icon": "image/dictionary.png"
  },
  "background": {
    "scripts": ["jquery-1.9.1.min.js", "background.js"]
  }, 
  "content_scripts": [
        {
            "all_frames": true,
            "js": [ "jquery-1.9.1.min.js", "content.js" ],
            "matches": [ "http://*/*", "https://*/*" ]
        }
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}